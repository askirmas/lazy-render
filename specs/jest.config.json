{
  "clearMocks": false,
  "collectCoverage": false,
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(t|j)sx?$",
  "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
  },
  "setupFilesAfterEnv": ["expect-puppeteer"],
  "preset": "jest-puppeteer",
  "verbose": true,
  "bail": 1
}