{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": false,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "baseUrl": "../node_modules",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
    ],
    "types": [
      "node",
      "cypress",
      "cypress-plugin-snapshots"
    ],
    "noEmit": true
  }
}